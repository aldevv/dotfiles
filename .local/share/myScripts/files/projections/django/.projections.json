{
  "*": {
    "console": ".venv/bin/python manage.py shell",
    "start": ".venv/bin/python {project}/manage.py runserver",
    "testrunner":  "djangotest"
  },
  "*/serializers.py": {
    "alternate": ["{}/tests/test_endpoints.py"],
    "related": ["{}/views.py", "{}/utils.py"],
    "type": "seri"
  },
  "*/viewsets.py": {
    "alternate": ["{}/tests/test_endpoints.py"],
    "related": ["{}/views.py", "{}/utils.py"],
    "type": "viewsets"
  },
  "*/urls.py": {
    "alternate": ["{}/tests/test_endpoints.py"],
    "related": ["{}/views.py", "{}/utils.py"],
    "type": "urls"
  },
  "*/models.py": {
    "alternate": ["{}/tests/test_endpoints.py"],
    "type": "models"
  }
}
